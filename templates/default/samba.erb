[global]
workgroup = smb
netbios name = pi
security = user
log file = /var/log/samba/log.%m
log level = 3
server role = member server
username map = /etc/samba/smbusers
passdb backend = tdbsam
load printers = no



[code]
path = /code/
browseable = yes
writable = yes
comment = smb share
guest ok = no
valid users = <%= @smbuser %>
